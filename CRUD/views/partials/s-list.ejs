<div>
    <h3 class="text-2xl text-center font-mono bg-green-500 text-white py-2 mx-5"> Student list</h3>
    <div class="mt-5 p-5">
        <table class="table-auto text-center bg-green-300 text-xl w-[100%]">
            <tr class="text-[#222]">
                <th class="p-2">Id</th>
                <th class="p-2">Name</th>
                <th class="p-2">Age</th>
                <th class="p-2">Year</th>
            </tr>
            <% let i = 1 %>
            <% data.forEach((item) => { %>
                <tr class="hover:bg-gray-200 even:bg-white odd:bg-blue-100 bg-indigo-300/100">
                    <td class="p-2">
                        <%= i++ %>
                    </td>
                    <td class="p-2">
                        <%= item.name %>
                    </td>
                    <td class="p-2">
                        <%= item.age %>
                    </td>
                    <td class="p-2">
                        <%= item.year %>
                    </td>
                    <td> edit </td>
                </tr>
            <% }) %>
           
        </table>

    </div>
</div>